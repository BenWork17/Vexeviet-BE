{
  "name": "@vexeviet/database",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "prisma:seed": "dotenv -e ../../.env -- ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "dotenv -e ../../.env -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prisma": "^5.7.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "dotenv-cli": "^7.3.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3"
  }
}
